.pic-like-image{
  height: 16px;
  width: 16px;
  position: relative;
  vertical-align: middle;
}

.item-ans{ 
  width: 84%;
  display: flex; 
  justify-content: space-between;
  flex-direction: row;
  box-sizing: border-box;
  border-radius: 3px;
  margin: 20rpx auto;
}

::-webkit-scrollbar
{
  width: 0;
  height: 0;
  color: transparent;
}

.dataTime{
  width:160px;
  height:20px;
  z-index:999;
  font-size: 20rpx;
  color: #aaa;
}
